<Page x:Class="Primecanva.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Primecanva"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="Page2">

    <Grid>
        <Canvas HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="800" Background="White">
            <Label Content="History" Canvas.Left="10" Canvas.Top="10" Height="42" Width="111" FontSize="25"/>
            <TextBox Height="466" Canvas.Left="13" TextWrapping="Wrap" Text="" Canvas.Top="57" Width="774" TextChanged="TextBox_TextChanged" FontSize="25"/>
            <Button Content="Clear history" Canvas.Left="21" Canvas.Top="540" Width="159" Height="40" FontSize="20" Click="Button_Click"/>
            <Button Content="Back" Canvas.Left="200" Canvas.Top="540" Width="103" Height="40" FontSize="20" Click="Button_Click"/>
        </Canvas>

    </Grid>
</Page>
