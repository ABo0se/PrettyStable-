<Page x:Class="Primecanva.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Primecanva"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="Page2">

    <Grid>
        <Canvas HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="800" Background="White">
            <Label Content="History" Canvas.Left="10" Canvas.Top="10" Height="42" Width="111" FontSize="25"/>
            <Button Content="Back" Canvas.Left="234" Canvas.Top="502" Width="100" Height="36" FontSize="20" Click="GoPageone"/>
            <Button Content="Clear history" Canvas.Left="21" Canvas.Top="502" Width="193" Height="36" FontSize="20" Click="Button_Click"/>
            <TextBox Height="427" Canvas.Left="21" TextWrapping="Wrap" Canvas.Top="56" Width="735" FontSize="20" TextChanged="TextBox_TextChanged"/>
        </Canvas>

    </Grid>
</Page>
